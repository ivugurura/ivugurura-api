{
  "name": "ivugurura",
  "version": "1.0.0",
  "description": "Reformation and revival",
  "private": true,
  "engines": {
    "node": ">=20.17.0"
  },
  "main": "src/app.js",
  "scripts": {
    "_test": "NODE_ENV=test nyc mocha -r @babel/register --color --exit",
    "test": "yarn _test \"tests/**/*.test.js\"",
    "migrate": "sequelize db:migrate",
    "migrate-down": "sequelize db:migrate:undo",
    "seed-up": "sequelize db:seed:all",
    "seed-down": "sequelize db:seed:undo:all",
    "dev": "NODE_ENV=develop nodemon --exec babel-node src/app.js",
    "clean": "rimraf dist",
    "build": "NODE_ENV=production yarn clean && babel ./src --out-dir dist",
    "start": "ulimit -c 0; NODE_ENV=production node dist/app.js",
    "clean-staging": "rimraf dist-staging",
    "build-staging": "NODE_ENV=staging yarn clean-staging && babel ./src --out-dir dist-staging",
    "start-staging": "ulimit -c 0; NODE_ENV=staging node dist-staging/app.js",
    "compress": "tar zcvf build.tar.gz ./",
    "un-compress": "tar zxvf build.tar.gz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AJAkimana/ivugurura.git"
  },
  "keywords": [
    "node",
    "reformation"
  ],
  "author": "Reformation voice",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AJAkimana/ivugurura/issues"
  },
  "homepage": "https://github.com/AJAkimana/ivugurura#readme",
  "dependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@sendgrid/mail": "^7.2.2",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.5",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dropbox": "^10.34.0",
    "express": "4.19.2",
    "express-device": "^0.4.2",
    "express-session": "^1.18.1",
    "express-useragent": "^1.0.13",
    "helmet": "^4.1.1",
    "html-to-text": "^9.0.5",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^2.0.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.3",
    "pm2": "^5.4.3",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.2.2",
    "shelljs": "^0.8.4",
    "slugify": "^1.3.6",
    "socket.io": "^4.8.1",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.7.0",
    "faker": "^6.6.6",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0"
  }
}
